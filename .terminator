[global_config]
  borderless = True
  title_font = Ubuntu Mono Bold 11
  title_use_system_font = False
[keybindings]
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_darkness = 0.9
    background_type = transparent
    cursor_color = "#aaaaaa"
    font = Ubuntu Mono Bold 13
    icon_bell = False
    scrollback_lines = 10000
    show_titlebar = False
    use_system_font = False
